      "text/plain": [
       "         rating  eu_sales  jp_sales  na_sales  other_sales\n",
       "0             E     58.06      8.94     50.74        15.39\n",
       "1          E10+     26.16      4.46     33.23         7.83\n",
       "2             M     93.44      8.01     96.42        28.67\n",
       "3             T     34.07     14.78     38.95        11.33\n",
       "4  нет рейтинга     58.95     56.90     64.72        18.40"
      ]
     },
     "execution_count": 84,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "rating"
   ]
