    "eu_sales_genre_5 = data.groupby(['genre'])\\\n",
    "                       .mean()[['eu_sales']]\\\n",
    "                       .sort_values(by='eu_sales', ascending=False)\\\n",
    "                       .head(5)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 88,
   "metadata": {},
   "outputs": [],
   "source": [
    "eu_sales_genre['group_name'] = (\n",
    "    eu_sales_genre['genre']\n",
    "    .where(eu_sales_genre['genre']\n",
    "           .isin(eu_sales_genre_5.index), 'Другие')\n",
    ")    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 89,
   "metadata": {},
   "outputs": [],
   "source": [
    "eu_sales_genre_final = (\n",
    "    eu_sales_genre.pivot_table(index='group_name', values='eu_sales', aggfunc='mean')\n",
    "    \n",
    ")\n",
    "eu_sales_genre_final.columns=['eu_sales']"
   ]
