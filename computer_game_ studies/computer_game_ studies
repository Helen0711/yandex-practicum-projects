    "eu_sales_genre = data.groupby(['genre'])\\\n",
    "                       .mean()[['eu_sales']]\\\n",
    "                       .reset_index()\\\n",
    "                       .sort_values(by='eu_sales', ascending=False)\n",
    "                    "
   ]
